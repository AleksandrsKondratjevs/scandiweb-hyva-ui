<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <!-- Global carousel nav block - only output when referenced within other blocks -->
        <block name="pagebuilder.faq_multiple.tabs" template="Scandiweb_ContentTypes::faq-multiple-tabs.phtml"/>

        <!-- Render JS templates depending on PageBuilder CMS content rendered on a page -->
        <referenceBlock name="page-js-dependencies">
            <arguments>
                <argument name="blockOutputPatternMap" xsi:type="array">
                    <item name="page_builder.faq_multiple" xsi:type="array">
                        <item name="regex" xsi:type="string">/data-content-type="faq_multiple"/</item>
                        <item name="template" xsi:type="string">Scandiweb_ContentTypes::widgets/faq-multiple.phtml</item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
